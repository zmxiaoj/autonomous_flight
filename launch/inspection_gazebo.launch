<launch>
    <!-- 添加 octomap_server 节点 -->
    <node pkg="octomap_server" type="octomap_server_node" name="octomap_server">
        <param name="frame_id" type="string" value="map" />
        <!-- 配置点云话题 -->
        <remap from="cloud_in" to="/camera/depth/points" />
    </node>

	<rosparam file="$(find autonomous_flight)/cfg/inspection/inspection_param.yaml" />
	<node pkg="autonomous_flight" type="inspection_node" name="inspection_node" output="screen" />
</launch>